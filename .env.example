# Environment Variables Template
# Copy this to .env and fill in the values

# API Configuration
EXPO_PUBLIC_API_BASE_URL=https://api.nutrition-app.com/v1

# Google OAuth
EXPO_PUBLIC_GOOGLE_CLIENT_ID_IOS=your_ios_client_id_here
EXPO_PUBLIC_GOOGLE_CLIENT_ID_ANDROID=your_android_client_id_here
EXPO_PUBLIC_GOOGLE_CLIENT_ID_WEB=your_web_client_id_here

# Apple OAuth (iOS)
EXPO_PUBLIC_APPLE_SERVICE_ID=your_apple_service_id_here

# Analytics
EXPO_PUBLIC_ANALYTICS_ID=your_analytics_id_here

# Sentry (Error Tracking)
EXPO_PUBLIC_SENTRY_DSN=your_sentry_dsn_here

# Feature Flags
EXPO_PUBLIC_ENABLE_ANALYTICS=true
EXPO_PUBLIC_ENABLE_CRASH_REPORTING=true
